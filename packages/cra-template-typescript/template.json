{
  "package": {
    "dependencies": {
      "@apollo/client": "^3.2.5",
      "@graphql-codegen/cli": "^1.19.0",
      "@graphql-codegen/near-operation-file-preset": "^1.17.11",
      "@graphql-codegen/typescript": "^1.17.11",
      "@graphql-codegen/typescript-operations": "^1.17.8",
      "@graphql-codegen/typescript-react-apollo": "^2.0.7",
      "@testing-library/jest-dom": "^5.11.4",
      "@testing-library/react": "^11.1.0",
      "@testing-library/user-event": "^12.1.10",
      "@types/node": "^12.0.0",
      "@types/react": "^17.0.0",
      "@types/react-dom": "^17.0.0",
      "@types/jest": "^26.0.15",
      "apollo": "^2.31.0",
      "typescript": "^4.1.2",
      "web-vitals": "^1.0.1",
      "eslint-config-airbnb": "^18.2.0",
      "eslint-config-airbnb-typescript": "^12.0.0",
      "eslint-config-prettier": "^6.15.0",
      "eslint-plugin-prettier": "^3.1.4",
      "eslint-import-resolver-typescript": "^2.3.0",
      "prettier": "^2.1.2"
    },
    "eslintConfig": {
      "extends": ["react-app", "react-app/jest"]
    },
    "scripts": {
      "lint": "./node_modules/.bin/eslint --ext .js --ext .jsx --ext .ts --ext .tsx src",
      "lint:fix": "npm run lint --fix",
      "format": "./node_modules/.bin/prettier \"src/**/*.{js,jsx,ts,tsx,json}\" --write",
      "gql:schema:download": "gq http://swapi.apis.guru/ --introspect > src/graphql/schema.generated.graphql",
      "gql:codegen": "graphql-codegen --config ./src/graphql/codegen.yml",
      "gql:faker": "graphql-faker ./src/graphql/schema.graphql --open"
    }
  }
}
